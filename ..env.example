# .env.example

# Set to 'true' to require an image for animation creation
VITE_REQUIRE_IMAGE=false

# Set to 'false' to disable multiple emoji selection
VITE_MULTIPLE_EMOJI=true

# Set to 'false' to disable object detection
VITE_OBJECT_DETECTION=true

# Set to 'false' to disable style transfer
VITE_STYLE_TRANSFER=true

# Set the maximum number of frames for an animation
VITE_MAX_FRAMES=16

# Set the default number of frames for an animation
VITE_DEFAULT_FRAMES=8
